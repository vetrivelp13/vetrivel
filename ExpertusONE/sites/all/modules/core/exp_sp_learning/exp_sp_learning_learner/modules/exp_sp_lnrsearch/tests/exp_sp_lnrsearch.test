  public function testSearchClassViewBasic(){
  	$user = $this->getUserList(3);
  	$testcases = $this->test_case['class_basic'];
  	$usr = new stdClass();
  	$usr->name =  $user[0]->username;
  	$usr->pass_raw = 'welcome';
  	$this->drupalLogin($usr);
  	
  	// Refine filter check
  	$this->getCatalogPage();
  	$opt = $this->srcParam;
  	$this->getCatalogResult($opt);
  	foreach($testcases as $tcase){
  		list($test,$msg,$grp) = $tcase;
  		$this->assertRaw($test,$msg,$grp);
  	}
	  
  	//No Class check
  	$this->getCatalogPage();
  	$opt = $this->srcParam;
  	expDebug::dPrint("Optional Parameter --> ".print_r($opt,true),5);
  	$opt['title'] = 'Title_'.rand(100,10000).'_Not_Found';
  	$this->getCatalogResult($opt);
  	//$this->assertRaw('<div style="display: block;" id="no-records">Your search did not return any results</div>','No class found success','lnr_catalog_search');
  	$this->assertTrue(count($this->cId)==0,'No class found success','lnr_catalog_search');
  	
  	// Filter search check
  	$filters = $this->getParamFilterValues();
  	expDebug::dPrint("List of filters -- ".print_r($filters,true),1);
  	foreach($filters as $filter){
  		list($fType,$fValue,$check,$msg) = $filter;
  		$this->content= '';
  		$opt = null;
  		$this->getCatalogPage();
	  	$opt[$fType] = $fValue;
	  	$this->getCatalogResult($opt);
	  	$dom = new DomDocument();
	  	$dom->loadHTML($this->content);
	  	$parent = $dom->getElementById('paintContentResults');
	  	foreach($parent->childNodes as $node){
	  		$this->content = $dom->saveHTML($node);
	  		$this->assertRaw($check,$msg,'lnr_catalog_search');
	  	}
  	}
  }
